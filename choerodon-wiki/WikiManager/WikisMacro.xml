    <property>
      <code>{{velocity}}
##
## List all the wikis.
##
#set($discard = $xwiki.ssx.use('WikiManager.WikisMacro'))
##
#set ($wikis = $services.wiki.all)
##
#if (!$wikis || $wikis.size() == 0 )
{{translation key="platform.wiki.macro.noWikis"/}}

#end
{{html wiki='true'}}
&lt;ul class='xlist'>
  ##
  ## List wikis.
  ##
  #foreach($wiki in $wikis)
    ## We check that we have the view right on the wiki
    #if ($xwiki.hasAccessLevel('view', $xcontext.userReference, $wiki.mainPageReference))
      &lt;li class='xitem xunderline xhighlight workspace spWorkspaceItem'>
        &lt;div class='xitemcontainer'>
          &lt;div class='spWorkspaceName'>{{html}}&lt;a href="${xwiki.getURL($wiki.mainPageReference)}">${wiki.prettyName}&lt;/a>{{/html}}&lt;/div>  
          &lt;div class='xspacer'>&amp;nbsp;&lt;/div>
        &lt;/div>
      &lt;/li>
    #end
  #end
  ## Wiki creation.
  ##
  ## Only if the current action is not inline. If it's inline, then there is already a form, and if we render the second, nested one, the DOM is messed up in IE.
  #if(!$isGuest &amp;&amp; $hasCreateWiki &amp;&amp; "$!xcontext.action" != 'inline')
    &lt;li id='spWorkspaceCreateItem' class='xitem workspace'>
      &lt;div class='xitemcontainer'>
        #set ($createWikiDocumentReference = $services.model.createDocumentReference($xcontext.mainWikiName, 'WikiManager', 'CreateWiki'))
        &lt;div id='spWorkspaceCreateLabel' class='spWorkspaceName'>&lt;a href="$xwiki.getURL($createWikiDocumentReference)">{{translation key="platform.wiki.create.title"/}}&lt;/a>&lt;/div>
        &lt;div class='spacer'>&amp;nbsp;&lt;/div>
      &lt;/div>
    &lt;/li>
  #end
&lt;/ul>
{{/html}}
{{/velocity}}</code>
